{
  "name": "redaction",
  "version": "4.3.2",
  "description": "Reducers without constants and dispatch",
  "homepage": "https://github.com/pavelivanov/redaction#readme",
  "author": "Pavel Ivanov <grammka@gmail.com> (https://github.com/pavelivanov)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelivanov/redaction.git"
  },
  "keywords": [
    "redaction",
    "react",
    "redux",
    "action",
    "reducer",
    "creator",
    "constants",
    "dispatch"
  ],
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "prepublish": "npm run build",
    "clean": "rimraf ./lib",
    "dev": "babel -w ./src --out-dir ./lib",
    "build": "npm run clean && babel ./src --out-dir ./lib"
  },
  "dependencies": {
    "redux-immutablejs": "^0.0.8",
    "redux-thunk": "^2.1.0"
  },
  "peerDependencies": {
    "immutable": "^3.0.0",
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "redux": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "connected-react-router": "^4.5.0",
    "enzyme": "^2.9.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "jest": "^21.2.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "redux": "^3.7.2",
    "rimraf": "^2.5.0"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/docs/",
      "/examples/",
      "/images/",
      "/node_modules/"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./lib"
    ]
  }
}
